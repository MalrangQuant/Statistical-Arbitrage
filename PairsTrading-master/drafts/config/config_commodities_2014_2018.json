{
    "dataset": {
        "path": "data/etfs/pickle/commodity_ETFs_long_updated",
        "ticker_attribute": "Ticker",
        "training_initial_date": "01-01-2014",
        "training_final_date": "01-01-2017",
        "testing_initial_date": "01-01-2017",
        "testing_final_date": "01-01-2018",
        "data_source": "yahoo",
        "nan_threshold": 0
    },
    "PCA": {
        "N_COMPONENTS": [10,15]
    },
    "clustering": {
        "algo": "DBSCAN",
        "epsilon": 0.4,
        "min_samples": 2
    },
    "pair_restrictions": {
        "min_half_life": 5,
        "min_zero_crossings": 12,
        "p_value_threshold": 0.05,
        "hurst_threshold": 0.5
    },
    "trading": {
        "strategy": "kalman",
        "lookback_multiplier": 2,
        "entry_multiplier": 1,
        "exit_multiplier": 0
    },
    "trading_filter": {
        "active": 0,
        "name": "correlation",
        "filter_lookback_multiplier": 2,
        "lag": 1,
        "diff_threshold": 0
    },
    "output": {
        "filename": "summary/results.xlsx"
    }
}